<Window x:Class="ATCPresentationGUI.Connect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Connect to server" Height="150" Width="300" Loaded="Window_Loaded">
    <Grid>
        <Label Content="Enter Server Address" Height="28" HorizontalAlignment="Center" Margin="0,12,0,0" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Center" Margin="0,40,0,0" Name="textBoxServer" VerticalAlignment="Top" Width="230" KeyUp="textBoxServer_KeyUp" />
        <Button Content="Connect" Height="23" HorizontalAlignment="Center" Margin="0,76,0,0" Name="buttonConnect" VerticalAlignment="Top" Width="75" Click="buttonConnect_Click" />
    </Grid>
</Window>
